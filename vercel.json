{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/videos/(.*)",
            "dest": "/public/videos/$1"
        },
        {
            "src": "^/$",
            "dest": "/server.js"
        },
        {
            "src": "/login",
            "dest": "/server.js"
        },
        {
            "src": "/signup",
            "dest": "/server.js"
        },
        {
            "src": "/custom-onboarding",
            "dest": "/server.js"
        },
        {
            "src": "/dashboard",
            "dest": "/server.js"
        },
        {
            "src": "/workouts",
            "dest": "/server.js"
        },
        {
            "src": "/nutrition",
            "dest": "/server.js"
        },
        {
            "src": "/health-metrics",
            "dest": "/server.js"
        },
        {
            "src": "/meal-suggestions",
            "dest": "/server.js"
        },
        {
            "src": "/google-fit",
            "dest": "/server.js"
        },
        {
            "src": "/doctor-consult",
            "dest": "/server.js"
        },
        {
            "src": "/settings",
            "dest": "/server.js"
        },
        {
            "src": "/logout",
            "dest": "/server.js"
        },
        {
            "src": "/health",
            "dest": "/server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "BASE_URL": "https://fit-with-ai.vercel.app",
        "SESSION_SECRET": "your-secret-key-for-production",
        "VERCEL": "1"
    },
    "regions": ["bom1"],
    "public": true,
    "github": {
        "silent": true
    }
} 