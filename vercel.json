{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/videos/(.*)",
            "dest": "/public/videos/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "BASE_URL": "https://fit-with-ai.vercel.app",
        "SESSION_SECRET": "@session_secret",
        "SMTP_USER": "@smtp_user",
        "SMTP_PASS": "@smtp_pass",
        "VERCEL": "1"
    },
    "regions": ["bom1"],
    "public": true,
    "github": {
        "silent": true
    }
} 