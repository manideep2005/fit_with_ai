{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/videos/(.*)",
            "dest": "/public/videos/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/server.js",
            "headers": {
                "Cache-Control": "no-store, no-cache, must-revalidate"
            }
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "BASE_URL": "https://fit-with-ai.vercel.app",
        "SESSION_SECRET": "your-secret-key",
        "VERCEL": "1"
    },
    "regions": ["bom1"],
    "public": true,
    "github": {
        "silent": true
    }
} 