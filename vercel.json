{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/videos/(.*)",
            "dest": "/public/videos/$1"
        },
        {
            "src": "/api/test-email",
            "dest": "/api/test-email.js"
        },
        {
            "src": "/api/debug-env",
            "dest": "/api/debug-env.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "BASE_URL": "https://fit-with-57d4pcvcw-gonugunta-manideeps-projects.vercel.app",
        "APP_URL": "https://fit-with-57d4pcvcw-gonugunta-manideeps-projects.vercel.app",
        "SESSION_SECRET": "temporary-session-secret-key-12345",
        "SMTP_USER": "fitwitai18@gmail.com"
    },
    "regions": ["bom1"],
    "public": true,
    "github": {
        "silent": true
    }
} 